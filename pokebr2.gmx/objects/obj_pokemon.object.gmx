            <string>/// EXP &amp; LEVEL UP

var check = false;

xp += base_xp/60/60      *8;

while (xp &gt;= base_xp)
{
    lv++;
    
    xp -= base_xp;
    
    base_xp = power(base_xp, 1.2);
    
    check = true;
}

if (check)
{
    stat_set_hp_lvlup();
    stat_set_atk();
    stat_set_def();
    stat_set_spatk();
    stat_set_spdef();
    stat_set_spd();
    
    level_get_texutre(lv); 
    
    // Evolution
    if (evo_lv != "")
    {
        var hold = real(evo_lv);
        
        if (string(hold) == string(evo_lv))
        {
            // It's an integer.
            if (lv &gt;= hold)
                evolve = true;
        }
        else
        {
            // It's a string.
            if (chance(lv))
                evolve = true;
        }
    }
}
